#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/brian/work/ctx-core/ctx-core/tools/ctx-core-package-tools/bin/node_modules:/home/brian/work/ctx-core/ctx-core/tools/ctx-core-package-tools/node_modules:/home/brian/work/ctx-core/ctx-core/tools/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules:/home/brian/work/ctx-core/node_modules:/home/brian/work/node_modules:/home/brian/node_modules:/home/node_modules:/node_modules:/home/brian/work/ctx-core/ctx-core/packages/google-ui-svelte/node_modules:/home/brian/work/ctx-core/ctx-core/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/brian/work/ctx-core/ctx-core/tools/ctx-core-package-tools/bin/node_modules:/home/brian/work/ctx-core/ctx-core/tools/ctx-core-package-tools/node_modules:/home/brian/work/ctx-core/ctx-core/tools/node_modules:/home/brian/work/ctx-core/ctx-core/node_modules:/home/brian/work/ctx-core/node_modules:/home/brian/work/node_modules:/home/brian/node_modules:/home/node_modules:/node_modules:/home/brian/work/ctx-core/ctx-core/packages/google-ui-svelte/node_modules:/home/brian/work/ctx-core/ctx-core/packages/node_modules"
fi
if [ -x "$basedir//bin/sh" ]; then
  exec "$basedir//bin/sh"  "$basedir/../../../../tools/ctx-core-package-tools/bin/symlink_src_assets_to_dist.sh" "$@"
else
  exec /bin/sh  "$basedir/../../../../tools/ctx-core-package-tools/bin/symlink_src_assets_to_dist.sh" "$@"
fi
