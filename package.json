{
	"name": "@ctx-core/google-ui-svelte",
	"version": "1.0.5",
	"description": "ctx-core google",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/google-ui-svelte#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/google-ui-svelte/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/google-ui-svelte.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "tsc src/index.ts --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.44",
		"@ctx-core/error": "workspace:^11.1.13",
		"@ctx-core/object": "workspace:^22.0.4"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.91",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/google.analytics": "^0.0.42",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.3",
		"svelte-preprocess": "^4.10.0",
		"svelte2tsx": "^0.4.11",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}
